            </dependency>

            <!-- 权限范围 -->
            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-orm</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- 多数据源 -->
            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-datasource</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- excel导入导出-->
            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-excel</artifactId>
                <version>${revision}</version>
