                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-dubbo</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- 缓存服务 -->
            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-redis</artifactId>
                <version>${revision}</version>
