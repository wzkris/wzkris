            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-statemachine</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- 接口模块 -->
            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-swagger</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- web容器-->
            <dependency>
                <groupId>com.wzkris</groupId>
                <artifactId>wzkris-common-web</artifactId>
                <version>${revision}</version>
            </dependency>

